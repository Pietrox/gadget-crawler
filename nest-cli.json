{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/crawler/src",
  "monorepo": true,
  "root": "apps/crawler",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/crawler/tsconfig.app.json"
  },
  "projects": {
    "crawler": {
      "type": "application",
      "root": "apps/crawler",
      "entryFile": "main",
      "sourceRoot": "apps/crawler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/crawler/tsconfig.app.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "datahub": {
      "type": "application",
      "root": "apps/datahub",
      "entryFile": "main",
      "sourceRoot": "apps/datahub/src",
      "compilerOptions": {
        "tsConfigPath": "apps/datahub/tsconfig.app.json"
      }
    },
    "interfaces": {
      "type": "library",
      "root": "libs/interfaces",
      "entryFile": "index",
      "sourceRoot": "libs/interfaces/src",
      "compilerOptions": {
        "tsConfigPath": "libs/interfaces/tsconfig.lib.json"
      }
    },
    "dto": {
      "type": "library",
      "root": "libs/dto",
      "entryFile": "index",
      "sourceRoot": "libs/dto/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dto/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "enums": {
      "type": "library",
      "root": "libs/enums",
      "entryFile": "index",
      "sourceRoot": "libs/enums/src",
      "compilerOptions": {
        "tsConfigPath": "libs/enums/tsconfig.lib.json"
      }
    },
    "factories": {
      "type": "library",
      "root": "libs/factories",
      "entryFile": "index",
      "sourceRoot": "libs/factories/src",
      "compilerOptions": {
        "tsConfigPath": "libs/factories/tsconfig.lib.json"
      }
    },
    "schemas": {
      "type": "library",
      "root": "libs/schemas",
      "entryFile": "index",
      "sourceRoot": "libs/schemas/src",
      "compilerOptions": {
        "tsConfigPath": "libs/schemas/tsconfig.lib.json"
      }
    },
    "helpers": {
      "type": "library",
      "root": "libs/helpers",
      "entryFile": "index",
      "sourceRoot": "libs/helpers/src",
      "compilerOptions": {
        "tsConfigPath": "libs/helpers/tsconfig.lib.json"
      }
    }
  }
}